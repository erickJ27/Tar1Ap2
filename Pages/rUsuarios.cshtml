@page
@model WebApplication2.rUsuariosModel
@{
    ViewData["Title"] = "rUsuarios";
}

<h1>Registro de usuario</h1>
    
    <div class="form-group row">

        <div class="ancho">
            <label for="Id">Id:</label>
            <input type="number" class="form-control " id="Id">
            <button type="button" class="btn btn-primary float-left ">Buscar</button>

        </div>
    </div>
    <div class="form-group row">
        
        <div class="col col-xs-4">
            <label for="Nom">Nombre:</label>
            <input type="text" class="form-control" id="Nombre">
        </div>
    </div>
    <div class="form-group row">
        <div class="col-xs-4">
            <label for="Usu">Usuario:</label>
            <input type="text" class="form-control" id="Usuario">
        </div>
    </div>
    <div class="form-group row ">
        <div class="col-xs-4">
            <label for="Correo">Correo:</label>
            <input type="email" class="form-control" id="Usuario">
        </div>
    </div>

    <div class="form-group row">
        <div class="col-xs-4">
            <label for="Contrasena">Contrasena:</label>
            <input type="password" class="form-control" id="con">
        </div>
    </div>
    <div class="form-group row">
        <div class="col-xs-4">
            <label for="Telefono">Telefono:</label>
            <input type="text" class="form-control" id="tel">
        </div>
    </div>
    <div class="form-group row">
        <div class="col-xs-4">
            <label for="FechaNacimiento">Fecha de Nacimiento:</label>
            <input type="datetime" class="form-control" id="fechaN">
        </div>
    </div>
    <div class="form-group row">
        <div class="col-xs-4">
            <label for="FechadeIngreso">Fecha de Ingreso:</label>
            <input type="datetime" class="form-control" id="fechaI">
        </div>
    </div>
    
    
